// For loop
for (var i = 0; i < 10; i = i + 1) print i;

// Nested for loop
for (var i = 0; i < 3; i = i + 1) {
    for (var j = 0; j < 2; j = j + 1) {
        print i + ", " + j;
    }
}

// While loop
var i = 0;
while (i < 10) {
    print i;
    i = i + 1;
}

// Nested while loop
var i = 0;
while (i < 3) {
    var j = 0;
    while (j < 2) {
        print i + ", " + j;
        j = j + 1;
    }
    i = i + 1;
}
